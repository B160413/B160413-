/* pages/result/result.wxss */
page {
  background-color: #fdfdfd;
  height: 100%;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
}

.view-page {
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  box-sizing: border-box;
  position: absolute;
  overflow-y: auto;
  /*overflow: hidden;*/
}

.page-content {
  box-sizing: border-box;
  position: relative;
  overflow-y: auto;
}



.break{
  /*width: 80%;*/
  word-wrap:break-word;
  overflow : hidden;    
  text-overflow: ellipsis;   
  display: -webkit-box;    
  -webkit-line-clamp: 2;   
  -webkit-box-orient: vertical;    
  word-break: break-all; /* 追加这一行代码 */
}
