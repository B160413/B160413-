page {
  /*background: #abd8e4;*/
  background: #ffffff;
  height: 100%;/*生效*/
  position: relative;
  /*top:-18px;*/
  overflow: hidden;
  box-sizing: border-box;
}

.view-page {
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  box-sizing: border-box;
  position: absolute;
  /*overflow: hidden;*/
  overflow-y: auto;
}

.page-content {
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  position: relative;
  overflow-y: auto;
  /*background: #abd8e4;*/
  background: #ffffff;
}